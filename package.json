{
  "name": "sharc-js",
  "private": false,
  "version": "1.4.1-dev1",
  "type": "module",
  "files": [
    "dist/*"
  ],
  "exports": {
    "./Animation": {
      "import": {
        "default": "./dist/Animation.js",
        "types": "./dist/types/Animation.js"
      }
    },
    "./Stage": {
      "import": {
        "default": "./dist/Stage.js",
        "types": "./dist/types/Stage.js"
      }
    },
    "./Sprites": {
      "import": {
        "default": "./dist/Sprites.js",
        "types": "./dist/types/Sprites.js"
      }
    },
    "./Channels": {
      "import": {
        "default": "./dist/Channels.js",
        "types": "./dist/types/Channels.js"
      }
    },
    "./Utils": {
      "import": {
        "default": "./dist/Utils.js",
        "types": "./dist/types/Utils.js"
      }
    },
    "./async_stages/OffscreenStage": {
      "import": {
        "default": "./dist/async_stages/OffscreenStage.js",
        "types": "./dist/async_stages/OffscreenStage.js"
      }
    },
    "./async_stages/WorkerStage": {
      "import": {
        "default": "./dist/async_stages/WorkerStage.js",
        "types": "./dist/async_stages/WorkerStage.js"
      }
    },
    "./AnimationUtils": {
      "import": {
        "default": "./dist/AnimationUtils.js",
        "types": "./dist/types/AnimationUtils.js"
      }
    },
    "./types/Animation": {
      "import": {
        "default": "./dist/types/Animation.js"
        "types": "./dist/types/Animation.js"
      }
    },
    "./types/Common": {
      "import": {
        "default": "./dist/types/Common.js",
        "types": "./dist/types/Common.js"
      }
    },
    "./types/Channels": {
      "import": {
        "default": "./dist/types/Channels.js",
        "types": "./dist/types/Channels.js"
      }
    },
    "./types/Events": {
      "import": {
        "default": "./dist/types/Events.js",
        "types": "./dist/types/Events.js"
      }
    },
    "./types/Sprites": {
      "import": {
        "default": "./dist/types/Sprites.js",
        "types": "./dist/types/Sprites.js"
      }
    },
    "./types/Stage": {
      "import": {
        "default": "./dist/types/Stage.js",
        "types": "./dist/types/Stage.js"
      }
    },
  },
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "hostdev": "vite --host",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "fix": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "preview": "vite preview --host"
  },
  "devDependencies": {
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "@vitejs/plugin-react": "^4.0.1",
    "eslint": "^8.44.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "prettier": "^3.2.4",
    "preview": "^0.1.3",
    "typescript": "^5.1.3",
    "vite": "^4.4.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gitpushjoe/sharc.git"
  }
}

